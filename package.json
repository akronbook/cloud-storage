  "files": [
    "dist"
  ],
  "version": "0.0.3",
  "main": "./dist/cloud-storage.umd.js",
  "module": "./dist/cloud-storage.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
